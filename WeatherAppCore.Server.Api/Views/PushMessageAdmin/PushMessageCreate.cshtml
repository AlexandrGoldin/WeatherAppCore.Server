
@using PresentationL.Models;
@model PushMessageEditModel;

    <title>PushMessageCreate</title>

<h3>Добавить сообщение</h3>

<form asp-action="PushMessageCreate" asp-controller="PushMessageAdmin">
    <div class="form-group">
        <label asp-for="MesCooling" class="control-label">Сообщение о похолодании</label>
        <input type="text" asp-for="MesCooling" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MesWarming" class="control-label">Сообщение о потеплении</label>
        <input type="text" asp-for="MesWarming" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
