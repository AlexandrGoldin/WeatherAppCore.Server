@using PresentationL.Models;
@model CityEditModel;

    <title>CityEditor</title>

<h3>Редактировать город</h3>

<form asp-action="CityEditor" asp-controller="CityAdmin" asp-route-Id="@Model.Id">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Город</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="IdCity" class="control-label">ID Города</label>
        <input type="text" asp-for="IdCity" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
