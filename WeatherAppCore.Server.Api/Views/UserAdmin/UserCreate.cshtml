
@using PresentationL.Models;
@model UserEditModel;

<title>UserCreate</title>

<h3>Добавление пользователя</h3>

<form asp-action="UserCreate" asp-controller="UserAdmin"> 
    <div class="form-group">
        <label asp-for="UserIdCity" class="control-label">ID Города</label>
        <input type="text" asp-for="UserIdCity" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UserNameCity" class="control-label">Город</label>
        <input type="text" asp-for="UserNameCity" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UserKey" class="control-label">Ключ пользователя</label>
        <input type="text" asp-for="UserKey" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DeviceName" class="control-label">Название устройства</label>
        <input type="text" asp-for="DeviceName" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>